{
  "name": "photer-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:production": "pnpm build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:carousel": "jest --testPathPatterns=\"Carousel|PostItem\" --coverage --watchAll=false",
    "test:carousel:watch": "jest --testPathPatterns=\"Carousel|PostItem\" --watch",
    "test:unit": "jest --coverage --watchAll=false",
    "test:integration": "jest --testPathPatterns=\"integration\" --coverage --watchAll=false",
    "test:accessibility": "jest --testPathPatterns=\"a11y|accessibility\" --watchAll=false",
    "test:performance": "jest --testPathPatterns=\"performance\" --watchAll=false",
    "test:quality": "jest --testPathPatterns=\"Carousel|PostItem|LogIn\\.test\\.tsx|ConditionalSidebarWrapper\" --coverage --watchAll=false",
    "test:quality:watch": "jest --testPathPatterns=\"Carousel|PostItem|LogIn\\.test\\.tsx|ConditionalSidebarWrapper\" --watch",
    "test:broken": "jest --testPathPatterns=\"useLogInForm|useSidebarVisibility\" --watchAll=false || true",
    "quality:check": "pnpm lint && pnpm test:unit && pnpm test:integration",
    "quality:full": "pnpm quality:check && pnpm test:e2e && pnpm test:accessibility",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:carousel": "playwright test --grep \"Carousel Functionality\"",
    "lint:md": "markdownlint '**/*.md'",
    "lint:md:fix": "markdownlint --fix '**/*.md'",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.0",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.5",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.1",
    "@reduxjs/toolkit": "^2.5.1",
    "@tailwindcss/postcss": "^4.0.1",
    "async-mutex": "^0.5.0",
    "clsx": "^2.1.1",
    "country-state-city": "^3.2.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.10.5",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.3.2",
    "react": "^19.0.0",
    "react-country-state-city": "^1.1.12",
    "react-datepicker": "^8.4.0",
    "react-dom": "^19.0.0",
    "react-easy-crop": "^5.4.1",
    "react-hook-form": "^7.49.0",
    "react-redux": "^9.2.0",
    "react-select": "^5.10.1",
    "react-toastify": "^11.0.3",
    "swiper": "^11.2.6",
    "tailwind-merge": "^3.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@playwright/test": "^1.55.0",
    "jest-axe": "^9.0.0",
    "jest-extended": "^4.0.2",
    "@storybook/addon-essentials": "8.5.3",
    "@storybook/addon-interactions": "8.5.3",
    "@storybook/addon-onboarding": "8.5.3",
    "@storybook/blocks": "8.5.3",
    "@storybook/nextjs": "8.5.3",
    "@storybook/react": "8.5.3",
    "@storybook/test": "8.5.3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/grecaptcha": "^3.0.9",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-compiler": "19.0.0-beta-27714ef-20250124",
    "eslint-plugin-storybook": "^0.11.2",
    "husky": "^9.1.7",
    "jest": "^30.1.2",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.1.6",
    "markdownlint-cli": "^0.45.0",
    "postcss": "^8.5.1",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "8.5.3",
    "tailwindcss": "^4.0.1",
    "typescript": "^5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "core-js-pure",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ],
    "overrides": {
      "next": "^15.2.3",
      "image-size": "^1.2.1",
      "esbuild": "^0.25.0",
      "@babel/runtime": "^7.26.10",
      "@babel/helpers": "^7.26.10"
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
