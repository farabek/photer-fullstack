    "@nestjs/swagger": "^11.0.3",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.15.0",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.12",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.7",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "buffer-to-stream": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "cross-env": "^7.0.3",
