{
  "name": "photer-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "build:all": "nest build",
    "build:gateway": "nest build gateway",
    "build:storage": "nest build storage",
    "start:gateway": "nest start gateway",
    "start:storage": "nest start storage",
    "start:dev:gateway": "cross-env ENV_TYPE=development nest start gateway --watch",
    "start:dev:storage": "cross-env ENV_TYPE=development nest start storage --watch",
    "start:debug:gateway": "cross-env ENV_TYPE=development nest start gateway --debug --watch",
    "start:prod:gateway": "node dist/apps/gateway/main",
    "start:prod:storage": "node dist/apps/storage/main",
    "migrate:dev": "npx prisma migrate deploy",
    "migrate:prod": "npx prisma migrate deploy",
    "prisma:studio": "npx prisma studio --schema=./apps/gateway/prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=./apps/gateway/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=./apps/gateway/prisma/schema.prisma",
    "prisma:reset": "npx prisma migrate reset --schema=./apps/gateway/prisma/schema.prisma",
    "prisma:deploy": "npx prisma migrate deploy --schema=./apps/gateway/prisma/schema.prisma",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest --coverage --watchAll=false",
    "test:integration": "jest --testPathPatterns=\"integration\" --coverage --watchAll=false",
    "test:security": "jest --testPathPatterns=\"security\" --watchAll=false",
    "test:load": "echo 'Load testing setup required - see TESTING.md'",
    "test:contracts": "jest --testPathPatterns=\"contracts\" --watchAll=false",
    "test:critical": "jest --testPathPatterns=\"critical\" --coverage --watchAll=false",
    "quality:check": "npm run lint && npm run test:unit && npm run test:integration",
    "quality:full": "npm run quality:check && npm run test:e2e:gateway && npm run test:security",
    "test:e2e:gateway": "jest --config ./apps/gateway/test/jest-e2e.json",
    "test:e2e:storage": "jest --config ./apps/storage/test/jest-e2e.json --detectOpenHandles",
    "db:reset": "prisma migrate reset",
    "db:seed": "ts-node --project apps/gateway/prisma/tsconfig.json apps/gateway/prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.782.0",
