    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.0",
    "streamifier": "^0.1.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
